    "wagmi": "^2.12.25"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@amplitude/analytics-browser": "^2.11.2",
    "@amplitude/plugin-session-replay-browser": "^1.6.24",
    "@internationalized/date": "^3.7.0",
    "@reown/appkit": "^1.2.0",
    "@reown/appkit-adapter-wagmi": "^1.2.0",
    "@stripe/stripe-js": "^4.1.0",
    "drawer-svelte": "^0.1.3",
