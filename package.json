{
  "name": "san-webkit-next",
  "version": "0.0.1",
  "author": "DmitriVanGuard <dimavanguard@gmail.com>",
  "lint-staged": {
    "*.{js,ts,svelte,css,md,mdx}": [
      "prettier --plugin-search-dir . --write"
    ],
    "*.{js,ts,svelte}": [
      "eslint --fix",
      "npm run check"
    ]
  },
  "bin": {
    "patch": "./scripts/patch.js"
  },
  "scripts": {
    "dev": "npm run storybook",
    "dev:https": "node ./scripts/mkcert.js && npm run storybook --https --ssl-cert ./mkcert/cert.pem --ssl-key ./mkcert/dev.pem",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.check.json --fail-on-warnings",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.check.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 4000 --ci --disable-telemetry",
    "build-storybook": "storybook build",
    "lib:release": "node scripts/release.js --run",
    "prepare": "husky"
  },
  "exports": {
    "./app.css": "./dist/app.css",
    "./ui/*": {
      "types": "./dist/ui/*.d.ts",
      "svelte": "./dist/ui/*.js"
    },
    "./scripts/*": {
      "types": "./scripts/*.d.ts",
      "default": "./scripts/*.js"
    },
    "./plugins/*": {
      "types": "./plugins/*.d.ts",
      "default": "./plugins/*.js"
    },
    "./api/*": "./dist/api/*"
  },
  "files": [
    "dist",
    "plugins",
    "scripts",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "graphql": "^16.8.2",
    "rxjs": "^7.8.1",
    "svelte": "^5.0.0-next.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.5",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@melt-ui/svelte": "^0.83.0",
    "@playwright/test": "^1.28.1",
    "@storybook/addon-essentials": "^8.2.7",
    "@storybook/addon-interactions": "^8.2.7",
    "@storybook/addon-links": "^8.2.7",
    "@storybook/addon-themes": "^8.2.7",
    "@storybook/blocks": "^8.2.7",
    "@storybook/manager-api": "^8.1.9",
    "@storybook/svelte": "^8.2.7",
    "@storybook/sveltekit": "^8.2.7",
    "@storybook/theming": "^8.1.9",
    "@sveltejs/adapter-auto": "^3.2.4",
    "@sveltejs/kit": "^2.5.25",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "2.43.0",
    "estree-walker": "^3.0.3",
    "globals": "^15.9.0",
    "graphql": "^16.8.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "magic-string": "^0.30.10",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "publint": "^0.1.9",
    "rxjs": "^7.8.1",
    "storybook": "^8.2.9",
    "svelte": "5.0.0-next.242",
    "svelte-check": "^3.8.6",
    "svg-sprite": "^2.0.4",
    "svgo": "^3.3.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-mkcert": "^1.17.5",
    "vitest": "^1.2.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@internationalized/date": "^3.5.4",
    "@stripe/stripe-js": "^4.1.0",
    "bits-ui": "^0.21.13",
    "esm-env": "^1.0.0",
    "fast-glob": "^3.3.2",
    "svelte-runes": "^0.0.4",
    "svelte-sonner": "^0.3.27",
    "tailwind-variants": "^0.2.1",
    "virtua": "^0.31.0",
    "zod": "^3.23.8"
  }
}
