{
  "name": "san-webkit-next",
  "version": "0.0.1",
  "author": "DmitriVanGuard <dimavanguard@gmail.com>",
  "lint-staged": {
    "*.{js,ts,svelte,css,md,mdx}": [
      "prettier --plugin-search-dir . --write"
    ],
    "*.{js,ts,svelte}": [
      "eslint --fix"
    ]
  },
  "bin": {
    "patch": "./scripts/patch.js"
  },
  "scripts": {
    "dev": "npm run storybook",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 4000 --ci --disable-telemetry",
    "build-storybook": "storybook build",
    "lib:release": "node scripts/release.js --run"
  },
  "exports": {
    "./ui/utils": {
      "types": "./dist/ui/utils/index.d.ts",
      "svelte": "./dist/ui/utils/index.js"
    },
    "./ui/core/Button": {
      "types": "./dist/ui/core/Button/index.d.ts",
      "svelte": "./dist/ui/core/Button/index.js"
    },
    "./ui/core/Calendar": {
      "types": "./dist/ui/core/Calendar/index.d.ts",
      "svelte": "./dist/ui/core/Calendar/index.js"
    },
    "./ui/core/Input": {
      "types": "./dist/ui/core/Input/index.d.ts",
      "svelte": "./dist/ui/core/Input/index.js"
    },
    "./ui/core/Checkbox": {
      "types": "./dist/ui/core/Checkbox/index.d.ts",
      "svelte": "./dist/ui/core/Checkbox/index.js"
    },
    "./ui/core/Dialog": {
      "types": "./dist/ui/core/Dialog/index.d.ts",
      "svelte": "./dist/ui/core/Dialog/index.js"
    },
    "./ui/core/Popover": {
      "types": "./dist/ui/core/Popover/index.d.ts",
      "svelte": "./dist/ui/core/Popover/index.js"
    },
    "./ui/app/AssetLogo": {
      "types": "./dist/ui/app/AssetLogo/index.d.ts",
      "svelte": "./dist/ui/app/AssetLogo/index.js"
    },
    "./ui/core/Svg": {
      "types": "./dist/ui/core/Svg/index.d.ts",
      "svelte": "./dist/ui/core/Svg/index.js"
    },
    "./ui/app/VirtualList": {
      "types": "./dist/ui/app/VirtualList/index.d.ts",
      "svelte": "./dist/ui/app/VirtualList/index.js"
    },
    "./ui/app/Picture": {
      "types": "./dist/ui/app/Picture/index.d.ts",
      "svelte": "./dist/ui/app/Picture/index.js"
    },
    "./postcss.config.js": {
      "types": "./postcss.config.d.ts",
      "default": "./postcss.config.js"
    },
    "./scripts/patch.js": {
      "types": "./scripts/patch.d.ts",
      "default": "./scripts/patch.js"
    },
    "./scripts/fetch.js": {
      "types": "./scripts/fetch.d.ts",
      "default": "./scripts/fetch.js"
    },
    "./scripts/asset-logos.js": {
      "types": "./scripts/asset-logos.d.ts",
      "default": "./scripts/asset-logos.js"
    },
    "./plugins/tailwind.js": {
      "types": "./plugins/tailwind.d.ts",
      "default": "./plugins/tailwind.js"
    },
    "./scripts/svg.js": {
      "types": "./scripts/svg.d.ts",
      "default": "./scripts/svg.js"
    },
    "./plugins/svelte.js": {
      "types": "./plugins/svelte.d.ts",
      "default": "./plugins/svelte.js"
    },
    "./tailwind.config.js": {
      "types": "./tailwind.config.d.ts",
      "default": "./tailwind.config.js"
    },
    "./scripts/utils.js": {
      "types": "./scripts/utils.d.ts",
      "default": "./scripts/utils.js"
    },
    "./plugins/vite.js": {
      "types": "./plugins/vite.d.ts",
      "default": "./plugins/vite.js"
    },
    "./scripts/release.js": {
      "types": "./scripts/release.d.ts",
      "default": "./scripts/release.js"
    },
    "./svelte.config.js": {
      "types": "./svelte.config.d.ts",
      "default": "./svelte.config.js"
    },
    "./ui/*": {
      "types": "./dist/ui/*.d.ts",
      "svelte": "./dist/ui/*.js"
    },
    "./scripts/*": {
      "types": "./scripts/*.d.ts",
      "default": "./scripts/*.js"
    },
    "./plugins/*": {
      "types": "./plugins/*.d.ts",
      "default": "./plugins/*.js"
    }
  },
  "files": [
    "dist",
    "plugins",
    "scripts",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "graphql": "^16.8.2",
    "rxjs": "^7.8.1",
    "svelte": "^5.0.0-next.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.5",
    "@melt-ui/svelte": "^0.81.0",
    "@playwright/test": "^1.28.1",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-themes": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/manager-api": "^8.1.9",
    "@storybook/svelte": "^8.1.9",
    "@storybook/sveltekit": "^8.1.9",
    "@storybook/theming": "^8.1.9",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/kit": "^2.5.11",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-svelte": "2.39.0",
    "estree-walker": "^3.0.3",
    "graphql": "^16.8.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "magic-string": "^0.30.10",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "publint": "^0.1.9",
    "rxjs": "^7.8.1",
    "storybook": "^8.1.9",
    "svelte": "5.0.0-next.154",
    "svelte-check": "^3.8.0",
    "svg-sprite": "^2.0.4",
    "svgo": "^3.3.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "vite": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@internationalized/date": "^3.5.4",
    "bits-ui": "^0.21.10",
    "esm-env": "^1.0.0",
    "fast-glob": "^3.3.2",
    "svelte-runes": "^0.0.3",
    "tailwind-variants": "^0.2.1",
    "virtua": "^0.31.0",
    "zod": "^3.23.8"
  }
}